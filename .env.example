# Google VertexAI
GOOGLE_GENAI_USE_VERTEXAI=true
GOOGLE_CLOUD_PROJECT=you-project
GOOGLE_CLOUD_LOCATION=us-central1

# Database Configuration (Cloud SQL PostgreSQL with IAM Authentication)
# Format: project:region:instance (e.g., my-project:us-central1:trackable-db)
INSTANCE_CONNECTION_NAME=your-project:us-central1:your-instance
DB_NAME=trackable
# DB_USER should be the service account email for IAM authentication
# Example: my-service-account@my-project.iam.gserviceaccount.com
DB_USER=your-service-account@your-project.iam.gserviceaccount.com

# Gmail API Configuration
USE_REAL_GMAIL_API=false
GMAIL_CLIENT_ID=your_client_id_here
GMAIL_CLIENT_SECRET=your_client_secret_here

# Carrier API Configuration
USE_REAL_CARRIER_APIS=false

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Cloud Tasks Configuration
CLOUD_TASKS_QUEUE=trackable-jobs
CLOUD_TASKS_LOCATION=us-central1
